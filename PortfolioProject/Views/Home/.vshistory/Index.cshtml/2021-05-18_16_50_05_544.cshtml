@model User

<div id="mouse-circle"></div>
<span class="navbar-toggler-icon"></span>
<nav class="navbar navbar-expand-md bg-dark navbar-dark" style="background: #110f0f;">
    <!-- Brand -->
    <a class="navbar-brand" href="#">
        <img src="" alt="Logo" style="width:40px;">
    </a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#collapsibleNavbar">

    </button>

    <!-- Navbar links -->
    <div class="collapse navbar-collapse d-flex justify-content-center align-content-center" id="collapsibleNavbar">
        <ul class="navbar-nav " >
            <li class="nav-item">
                <a class="nav-link">About</a>
            </li>
            <li class="nav-item">
                <a class="nav-link">Portfolio</a>
            </li>
            <li class="nav-item">
                <a asp-controller="Home" asp-action="Contact" class="nav-link">Contact Me</a>
            </li>
        </ul>
    </div>
</nav>
    <div>@Model.lastName</div>
    <div>@Model.firstName</div>
    <div>@Model.aboutInfo</div>
    <img src="@Model.profilePictureUrl" alt="Alternate Text" style="width:120px;" />
    @foreach (var project in Model.projects)
    {
        <h3>@project.ID</h3>
        <h3>@project.projectTitle</h3>
        <h3>@project.projectDescription</h3>
        <img src="@project.projectImageUrl" alt="Alternate Text" style="width:200px;" />
    }
