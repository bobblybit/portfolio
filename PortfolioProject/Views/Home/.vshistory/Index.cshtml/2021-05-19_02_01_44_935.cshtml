@model HomeViewModel

    <div class="row wrapper  mt-2 no-gutters">
        <div class="col-md-6 no-gutters">
            <div class="left-side">
                <div class="profile-img-wrapper mt-4">
                    <img src="@Model.profilePictureUrl"  alt="Alternate Text" style="width:100%; height:100%; border-radius:100%;" />
                </div>
                <div class="Hello-note">
                    Hello I am @Model.lastName @Model.firstName Proficient in
                    "Lorem ipsum dolor sit amet, consectetur adipiscing elit, sed do eiusmod tempor incididunt ut labore et
                    dolore magna aliqua. Ut enim ad minim veniam, quis nostrud exercitation ullamco laboris nisi ut aliquip ex
                    ea commodo consequat.
                    Duis aute irure dolor in reprehenderit in voluptate velit esse cillum dolore eu fugiat nulla pariatur.
                </div>
                <div>@Model.lastName</div>
                <div>@Model.firstName</div>
            </div>
        </div>
        <div class="col-md-6 no-gutters">
            <div class="right-side d-flex justify-content-center">
                <div class=" mt-5">@Model.tagLine</div>
            </div>
        </div>
    </div>



    <div>@Model.aboutInfo</div>


     @foreach (var Proficiency in Model.Proficiency)
     {
        <div>@Proficiency.name</div>
     }


    @foreach (var project in Model.projects)
    {
        <h3>@project.projectTitle</h3>
        <h3>@project.projectDescription</h3>
        <img src="@project.projectImageUrl" alt="Alternate Text" style="width:200px;" />
    }
