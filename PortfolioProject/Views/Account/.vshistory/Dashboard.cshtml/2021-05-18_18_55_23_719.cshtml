@{
    Layout = "_DashboardLayout";
}

<!-- Classic tabs -->
<div class="row">
    <div class="col-3">
        <div class="nav flex-column nav-pills" id="v-pills-tab" role="tablist" aria-orientation="vertical">
            <a class="nav-link active" id="v-pills-home-tab" data-toggle="pill" href="#v-pills-home" role="tab" aria-controls="v-pills-home" aria-selected="true">Home</a>
            <a class="nav-link" id="v-pills-profile-tab" data-toggle="pill" href="#v-pills-profile" role="tab" aria-controls="v-pills-profile" aria-selected="false">Profile</a>
        </div>
    </div>
    <div class="col-9">
        <div class="tab-content" id="v-pills-tabContent">
            <!-- pill profile -->
            <div class="tab-pane fade show active dash-tab-field" id="v-pills-home" role="tabpanel" aria-labelledby="v-pills-home-tab" style=" border: 1px solid;">
                <div></div>
            </div>
            <!-- pill Edit profile -->
            <div class="tab-pane fade dash-tab-field" id="v-pills-profile" role="tabpanel" aria-labelledby="v-pills-profile-tab" style=" border: 1px solid;">
                edit profile
            </div>
        </div>
    </div>
</div>
<!-- Classic tabs -->